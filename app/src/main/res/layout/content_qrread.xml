<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/content_qrread"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="xiaoliang.ltool.activity.QRReadActivity"
    tools:showIn="@layout/activity_qrread"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    android:background="@android:color/white">
    <SurfaceView
        android:id="@+id/activity_qrread_surface"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <xiaoliang.ltool.view.QRFinderView
        android:id="@+id/activity_qrread_finder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</FrameLayout>
